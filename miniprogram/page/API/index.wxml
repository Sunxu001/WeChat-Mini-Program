<set-tab-bar wx:if="{{isSetTabBarPage}}" bindunmount="leaveSetTabBarPage"></set-tab-bar>
<view wx:else class="index page" data-weui-theme="{{theme}}">
  <view class="index-hd">
  <!--用户登录-->
    <view class="login">
    <image src="../../image/profile- icon.png" mode="widthFix"/>
    <button>点击登陆</button>
    </view>
  <!--信息栏-->
<view class="user-info">
<button>
<image class="btn-icon" src="../../image/icon-Personal Info.png" mode="widthFix"/>
<text class="wtxt">个人信息</text>
<image class="arrow-btn" src="../../image/arrow.png" mode="widthFix"/>
</button>
<button>
<image  class="btn-icon"  src="../../image/icon-use.png" mode="widthFix"/>
<text class="wtxt" >使用说明</text>
<image class="arrow-btn" src="../../image/arrow.png" mode="widthFix"/>
</button>
<button>
<image  class="btn-icon"  src="../../image/icon-Qfeedback.png" mode="widthFix"/>
<text class="wtxt">问题反馈</text>
<image class="arrow-btn" src="../../image/arrow.png" mode="widthFix"/>
</button>
</view>
  </view>
</view>
